#include <iostream>

struct COMPLEX
{
	int z;
	int a = 3;
	int b = 4;
	int i = -1;

	int plus(COMPLEX)
	{
		return z = a + b * i;
	}
	int minus(COMPLEX)
	{
		return z = a - b * i;
	}
	//Деление не смог сделать
	int umnojenie(COMPLEX)
	{
		return z = (a * i) + (b * i);
	}
};


int main()
{
	COMPLEX comp;
	
}

