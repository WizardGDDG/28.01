#include <iostream>

struct CAR
{
	int length;
	int groundClearance;
	int engineCapacity;
	int enginePower;
	int wheelDiameter;
	std::string color;
	std::string transmissionType;

	void input(int newLength, int newGroundClearance, int newEngineCapacity, int newEnginePower, int newWheelDiameter, std::string newColor, std::string newTransmissionType)
	{
		length = newLength;
		groundClearance = newGroundClearance;
		engineCapacity = newEngineCapacity;
		enginePower = newEnginePower;
		wheelDiameter = newWheelDiameter;
		color = newColor;
		transmissionType = newTransmissionType;
	}
	void print()
	{
		std::cout << "Длина " << length << std::endl;
		std::cout << "Высота клиренса " << groundClearance << std::endl;
		std::cout << "Объем двигателя " << engineCapacity << std::endl;
		std::cout << "Мощность двигателя " << enginePower << std::endl;
		std::cout << "Диаметр колеса " << wheelDiameter << std::endl;
		std::cout << "Цвет " << color << std::endl;
		std::cout << "Тип коробки передач " << transmissionType << std::endl;
	}
};


int main()
{
	CAR car;
	
}

